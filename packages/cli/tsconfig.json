{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "declaration": true,
    "sourceMap": true,
    "outDir": "dist",
    "noEmitOnError": false,
    "resolveJsonModule": true
  },
  "include": ["src/**/*"],
  "exclude": ["**/*.spec.ts"],
  "lib": ["es2015"]
}
