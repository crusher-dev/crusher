{
    "isEnterprise": "<%= isEnterprise ? "true" : "false" %>",
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"target": "es5",
		"module": "esnext",
		"jsx": "preserve",
		"strict": true,
		"alwaysStrict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"noEmit": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"baseUrl": "../",
		"types": ["@emotion/react/types/css-prop"],
		"typeRoots": [",./@types", ",./node_modules/@types"],
		"paths": {
<% if(isEnterprise) { %>
			"@components/*": ["crusher-app/src_ee/ui/components/*", "crusher-app/src/ui/components/*"],
<% } else { %>
            "@components/*": ["crusher-app/src/ui/components/*"],
<% }%>
			"@pages/*": ["crusher-app/pages/*"],
			"@services/*": ["crusher-app/src/services/*"],
			"@ui/*": ["crusher-app/src/ui/*"],
			"@templates/*": ["crusher-app/src/templates/*"],
			"@utils/*": ["crusher-app/src/utils/*"],
			"@template/*": ["crusher-app/src/template/*"],
			"@redux/*": ["crusher-app/src/redux/*"],
			"@interfaces/*": ["crusher-app/src/interfaces/*"],
			"@hoc/*": ["crusher-app/src/hoc/*"],
			"@containers/*": ["crusher-app/src/containers/*"],
			"@constants/*": ["crusher-app/src/constants/*"],
			"@code-generator/*": ["code-generator/*"],
			"@crusher-shared/*": ["crusher-shared/*"],
			"@svg/*": ["crusher-app/src/svg/*"]
		}
	},
	"exclude": ["node_modules"],
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "**/*.svg", "../crusher-shared/constants/*.ts", "../crusher-shared/constants/*.tsx"]
}

