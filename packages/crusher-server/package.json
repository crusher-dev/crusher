	"scripts": {
		"setup": "node setup/tsconfig.js",
		"dev": "ts-node-dev --transpile-only true -r tsconfig-paths/register --project tsconfig.json src/app.ts",
		"dev:cron": "ts-node-dev --transpile-only true -r tsconfig-paths/register --project tsconfig.json src/cron.ts",
		"dev:queue": "ts-node-dev --transpile-only true -r tsconfig-paths/register  --project tsconfig.json src/queue.ts",
		"dev:storage": "ts-node-dev --transpile-only true -r tsconfig-paths/register --project tsconfig.json src/localFileStorageServer.ts",
		"run:prod": "NODE_ENV=production ts-node src/app.ts",
		"build:cron": "NODE_ENV=production ncc build src/cron.ts -o dist/cron --external bullmq",
		"build:queue": "NODE_ENV=production ncc build src/queue.ts -o dist/queue --external bullmq",
		"build": "NODE_OPTIONS=--max-old-space-size=8096 NODE_ENV=production webpack",
		"build:queue:start": "NODE_ENV=production node dist/queue/index.js",
		"build:cron:start": "NODE_ENV=production node dist/cron/index.js",
		"build:start": "NODE_ENV=production node dist/server/index.js",
		"lint": "prettier --write .",
		"lint:ts": "eslint --ext=jsx,ts,tsx src",
		"lint-fix:ts": "eslint --fix src",
		"prettify": "prettier --write ."
	}
