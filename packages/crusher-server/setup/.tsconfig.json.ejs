{
  "isEnterprise": "<%= isEnterprise ? "true" : "false" %>",
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": ["es5", "es6", "DOM"],
    "target": "ES2021",
    "module": "commonjs",
    "moduleResolution": "node",
    "baseUrl": "../",
    "outDir": "./build",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "inlineSources": true,
    "noEmitHelpers": false,
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "strictNullChecks": false,
    "esModuleInterop": false,
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitReturns": false,
    "isolatedModules": true,
    "keyofStringsOnly": true,
    "paths": {
      "@crusher-shared/*": ["crusher-shared/*"],
      "@constants": ["crusher-server/src/constants.ts"],
<% if(isEnterprise){ %>
    "@modules/*": ["crusher-server/src/ee/modules/*", "crusher-server/src/modules/*"],
    "@utils/*": ["crusher-server/src/ee/utils/*","crusher-server/src/utils/*"]
<% } else { %>
    "@modules/*": ["crusher-server/src/modules/*"],
    "@utils/*": ["crusher-server/src/utils/*"]
<% } %>
    }
  },
  "typeRoots": ["node_modules/@types"],
  "files": ["src/app.ts", "src/queue.ts", "src/localFileStorageServer.ts"],
  "include": ["./src/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"],
  "skipLibCheck": true,
  "ts-node": {
    "swc": true,
    "files": true
  }
}
