            .overlay_modal_content(data-recorder='true')
                #events_grid.overlay_grid(data-recorder='true')
                    .overlay_add_event_item(data-action='HOVER', data-recorder='true') Hover
                    .overlay_add_event_item(data-action='CLICK', data-recorder='true') Click
                    .overlay_add_event_item(data-action='SCROLL_TO_VIEW', data-recorder='true') Scroll to view
                    .overlay_add_event_item(data-action='EXTRACT_INFO', data-recorder='true') Extract Info
                    .overlay_add_event_item(data-action='ASSERT_TEXT', data-recorder='true') Assert Text
                    .overlay_add_event_item(data-action='SCREENSHOT', data-recorder='true') Screenshot
                #extract_event_form.form_container(data-recorder='true', data-gone='true')
                    input#extract_variable_name.modal_input(placeholder='Name of variable', data-recorder='true')
                    textarea#validation_script.form_textarea(data-recorder='true')
                    .form_label.label(data-recorder='true') Extracted Info
                    textarea#extracted_info_preview.form_textarea(placeholder='', disabled='true', data-recorder='true')
                    #submit_extract_form.form_submit_btn(data-recorder='true') Save
                #assert_event_form.form_container(data-recorder='true', data-gone='true')
                    .form_label.label(data-recorder='true') Should Equal to
                    .form_label
                        select#options(name='options')
                            option(value='equal') Equal
                            option(value='notEqual') Not Equal
                            option(value='isAtMost') isAtMost
                            option(value='isNull') isNull
                            option(value='isNotNull') isNotNull
                            option(value='isNan') isNan
                            option(value='isNotNaN') isNotNaN
                            option(value='isArray') isArray
                            option(value='isFinite') isFinite
                            option(value='include') include
                            option(value='include') include
                            option(value='match') match
                            option(value='notMatch') notMatch
                            option(value='lengthOf') lengthOf
                            option(value='oneOf') oneOf
                            option(value='isEmpty') isEmpty
                            option(value='isNotEmpty') isNotEmpty
                            option(value='isOk') isOk
                            option(value='array') array
                    input#assert_comparision_text.modal_input(placeholder='', data-recorder='true')
                    .form_label.label(data-recorder='true') Validation
                    input#assert_validation.modal_input(placeholder='Validation', disabled='true', data-recorder='true')
                    #submit_assert_form.form_submit_btn(data-recorder='true') Save
