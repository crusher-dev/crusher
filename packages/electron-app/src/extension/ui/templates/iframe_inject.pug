#overlay_add_action(data-recorder='true')
    #popper_arrow(data-popper-arrow='true')
    #overlay_add_icon.arrow_box(data-recorder='true')
        span#overlay_add(data-recorder='true') +
        #overlay_add_events_container(data-recorder='true')
            .overlay_heading_container(data-recorder='true')
                span.overlay_heading(data-recorder='true') Select action
                .overlay_close_icon(data-recorder='true')
