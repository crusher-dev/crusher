{
	"name": "test-runner",
	"version": "1.0.0",
	"description": "Crusher test runner used to execute and save output",
	"dependencies": {
		"@types/archiver": "^5.3.1",
		"@types/ioredis": "^4.17.11",
		"archiver": "^5.3.0",
		"aws-sdk": "^2.833.0",
		"axios": "^0.23.0",
		"bullmq": "1.67.1",
		"crusher-runner-utils": "0.0.9",
		"dotenv": "^10.0.0",
		"ioredis": "^4.19.4",
		"js-beautify": "^1.14.0",
		"logdna": "^3.5.0",
		"mongoose": "^5.11.14",
		"playwright": "1.23.0",
		"playwright-chromium": "1.23.0",
		"playwright-video": "https://github.com/crusherdev/playwright-video.git#f7dececde258b07bdec207e4bb6869d389655704",
		"shelljs": "^0.8.4",
		"ts-node": "^9.1.1",
		"uuid": "^8.3.2",
		"winston": "^3.7.2",
		"yargs": "^17.2.0"
	},
	"devDependencies": {
		"@types/bull": "^3.15.0",
		"@types/mongoose": "^5.10.3",
		"@types/node": "^14.14.22",
		"@types/shelljs": "^0.8.8",
		"@types/uuid": "^8.3.1",
		"@typescript-eslint/parser": "^4.14.1",
		"copy-webpack-plugin": "6.2.1",
		"prettier": "^2.2.1",
		"string-replace-loader": "^3.0.1",
		"tsconfig-paths-webpack-plugin": "^3.5.1",
		"typescript": "^4.1.3"
	},
	"scripts": {
		"setup": "node setup/tsconfig.js",
		"generate:local": "NODE_ENV=development ts-node -r tsconfig-paths/register  --project tsconfig.json scripts/runTestInHost.ts",
		"run:local": "NODE_ENV=development ts-node -r tsconfig-paths/register  --project tsconfig.json bin/out.ts",
		"start": "NODE_ENV=development  ts-node  -r tsconfig-paths/register  --project tsconfig.json  --transpile-only ./index.ts",
		"test": "ts-node -r tsconfig-paths/register --project tsconfig.json  out.ts",
		"tests": "ts-node -r tsconfig-paths/register --project tsconfig.json  code.ts",
		"start:cdp": "ts-node -r tsconfig-paths/register --project tsconfig.json ./test.ts",
		"build": "webpack -c webpack.config.js",
		"start:build": "node build/index.js",
		"lint": "prettier --write **/* && prettier --write **/**/* && prettier --write **/**/**/* && prettier --write *"
	},
	"author": "Utkarsh Dixit<utkarsh@crusher.dev>",
	"license": "ISC"
}
